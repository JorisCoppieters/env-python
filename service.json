{
    "$schema": "https://raw.githubusercontent.com/JorisCoppieters/servicerator/master/schemas/servicerator-schema-v3.4.json",
    "docker": {
        "container": {
            "ports": [
                {
                    "container": 8888,
                    "description": "Jupyter Notebook",
                    "host": 8888
                }
            ]
        },
        "image": {
            "name": "env-python",
            "tag_with_date": true,
            "version": "1.0.3"
        },
        "username": "joris"
    },
    "schema_version": 3.4,
    "version_control": {
        "root_folder": "$BASE_DIR",
        "type": "git"
    }
}